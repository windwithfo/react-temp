{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "styled-jsx/babel",
      {
        "optimizeForSpeed": true,
        "plugins": ["styled-jsx-plugin-less"]
      }
    ],
    ["import", {
      "libraryName": "antd",
        "style": "true"
      }
    ]
  ]
}
